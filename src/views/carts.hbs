<h1><strong>Cart</strong></h1>

<h3 style="color: green">{{messagge}}</h3>
<h3 style="color: brown">{{error}}</h3>

<hr>
<br>

<section>
    <h2>Welcome <span style="color: rgb(77, 77, 77)">{{user.first_name}}</span></h2>
    <br>
    <h3>Products In Cart</h3>

    {{#if cart}}
        <ul>
        {{#each cart}}
            <li>{{cart.products}}</li>
        {{/each}}
        </ul>

        {{else}}
        <h2>Not found products</h2>
    {{/if}}
</section>